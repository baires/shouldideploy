  "dependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@vercel/og": "^1.0.0",
    "date-fns": "^4.1.0",
    "next": "^15.3.3",
    "prettier-eslint-cli": "^8.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "24.0.3",
    "@types/react": "19.1.8",
    "babel-eslint": "^10.1.0",
    "eslint": "9.31.0",
    "eslint-config-next": "^15.3.2",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-react": "^7.37.0",
    "husky": "^9.1.5",
    "jest": "^30.0.0",
    "lint-staged": "^16.1.0",
    "prettier": "^3.6.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "5.8.2"
  },
  "husky": {
